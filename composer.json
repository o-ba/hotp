{
	"name": "o-ba/hotp",
	"description": "TYPO3 hmac-based one-time password provider",
	"type": "typo3-cms-extension",
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Oliver Bartsch",
			"email": "bo@cedev.de"
		}
	],
	"require": {
		"php": ">=7.4",
		"typo3/cms-core": "~11.1.0@dev",
		"bacon/bacon-qr-code": "^2.0",
		"christian-riesen/base32": "^1.5"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "hotp"
		}
	},
	"replace": {
		"typo3-ter/hotp": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Bo\\Hotp\\": "Classes/"
		}
	}
}
